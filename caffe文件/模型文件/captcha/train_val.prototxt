#########各任务根据fc8_i和label_i分别计算损失函数
#########多任务中各权重为1.0/5 = 0.2
layer {
  name: "loss_1"
  type: "SoftmaxWithLoss"
  bottom: "fc8_1"
  bottom: "label_1"
  top: "loss_1"
  ###权重
  loss_weight: 0.2
}
